{
  "name": "dasar-ui",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev --port 3000",
    "start": "node .output/server/index.mjs",
    "build": "vite build",
    "serve": "vite preview",
    "test": "vitest run",
    "check:lint": "eslint src --ext .ts,.tsx",
    "check:lint:fix": "eslint src --ext .ts,.tsx --fix",
    "check:format": "prettier --check \"**/*.{ts,tsx,mdx}\" --cache",
    "check:format:fix": "prettier --write \"**/*.{ts,tsx,mdx}\" --cache",
		"check:unused": "knip --max-issues 1",
		"check:unused:fix": "knip --fix"
  },
  "dependencies": {
    "@tanstack/react-router": "^1.130.2",
    "@tanstack/react-router-devtools": "^1.130.2",
    "@tanstack/react-router-with-query": "^1.130.2",
    "@tanstack/react-start": "^1.130.2",
    "@tanstack/router-plugin": "^1.121.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^3.7.2",
    "@tailwindcss/postcss": "^4.1.11",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.2.0",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@typescript-eslint/parser": "^8.39.0",
    "@vitejs/plugin-react-swc": "^4.0.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-react": "^7.32.2",
    "jsdom": "^26.0.0",
    "knip": "5.61.3",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.7.2",
    "vite": "^6.3.5",
    "vitest": "^3.0.5",
    "web-vitals": "^4.2.4"
  }
}
